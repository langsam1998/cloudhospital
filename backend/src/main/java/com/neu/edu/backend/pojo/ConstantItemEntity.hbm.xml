<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neu.edu.backend.pojo.ConstantItemEntity" table="constant_item" schema="cloudhospital">
        <id name="id">
            <column name="id" sql-type="varchar(11)" length="11"/>
        </id>
        <property name="constantTypeId">
            <column name="constant_type_id" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="constantCode">
            <column name="constant_code" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="constantName">
            <column name="constant_name" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="price">
            <column name="price" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>