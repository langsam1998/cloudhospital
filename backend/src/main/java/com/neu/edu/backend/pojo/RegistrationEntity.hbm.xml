<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neu.edu.backend.pojo.RegistrationEntity" table="registration" schema="cloudhospital">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="medicalHistoryId">
            <column name="medical_history_id" sql-type="int(11)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="idNumber">
            <column name="id_number" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="birthDate">
            <column name="birth_date" sql-type="date"/>
        </property>
        <property name="age">
            <column name="age" sql-type="int(11)"/>
        </property>
        <property name="ageType">
            <column name="age_type" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date"/>
        </property>
        <property name="morningAfternoon">
            <column name="morning_afternoon" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="departmentId">
            <column name="department_id" sql-type="int(11)"/>
        </property>
        <property name="doctorId">
            <column name="doctor_id" sql-type="int(11)"/>
        </property>
        <property name="registrationLevel">
            <column name="registration_level" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="paymentType">
            <column name="payment_type" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="needHistorybook">
            <column name="need_historybook" sql-type="varchar(1)" length="1"/>
        </property>
        <property name="registrationTime">
            <column name="registration_time" sql-type="datetime"/>
        </property>
        <property name="register">
            <column name="register" sql-type="varchar(11)" length="11"/>
        </property>
        <property name="state">
            <column name="state" sql-type="varchar(11)" length="11"/>
        </property>
    </class>
</hibernate-mapping>