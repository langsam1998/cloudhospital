<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.neu.edu.backend.pojo.ConstantTypeEntity" table="constant_type" schema="cloudhospital">
        <id name="id">
            <column name="id" sql-type="varchar(11)" length="11"/>
        </id>
        <property name="constantTypeCode">
            <column name="constant_type_code" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="constantTypeName">
            <column name="constant_type_name" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>